{
    "manifest_version" : 2,
    "name" : "Ad-bricoBlocker",
    "version" : "1.0",
    "description" : "Block ads and pop-ups",
    "icons" : {
        "48" : "icons/abricot.png"
    },
    "content_scripts" : [
        {
            "run_at": "document_start",
            "matches":["<all_urls>"],
            "js": ["main_blocker.js"]
        }
    ]
}